<div class="row">
	<% @listings.each do |listing| %>

	<div class="well col-xs-7 col-md-4">
			<%= youtube_thumb_embed(listing.video).html_safe %><br>
			<%= listing.description %><br>
			<strong><%= link_to "Watch This Video", listing.video, {class: "fancybox-media"} %></strong><br>
	</div>	
	<% end %>
</div>
