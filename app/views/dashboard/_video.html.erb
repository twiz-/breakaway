<div class="dasboard-listing">
	<% @listings.each do |listing| %>
	 <div class="row">
		 <div class="col-md-6 dash-game-list">
			  <%= youtube_thumb_embed_dash(listing.video).html_safe %>
			  <div class="pull-right">
				  <ul class="admin-dash-functions" style="text-align:right;">
					 <li><%= listing.description %> </li>
					 <li> <%= link_to "Remove", listing, method: :delete, data:{confirm: "This cannot be undone"} if current_player == listing.player %></li>
				  </ul>
			  </div>
		  </div> 
     </div>
    <% end %>
</div>