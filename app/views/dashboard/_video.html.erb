<div class="dasboard-listing">
	<% @listings.each do |listing| %>
	 <div class="row">
		 <div class="col-md-6 dash-game-list">
			  <img class="dash-thumb" src="http://i1.ytimg.com/vi/<%= youtube_thumb_embed(listing.video)%>/mqdefault.jpg">
			  <div class="pull-right">
				  <ul class="admin-dash-functions" style="text-align:right;">
					 <li><%= listing.description %> </li>
					 <li> <%= link_to "Remove", listing, method: :delete, data:{confirm: "This cannot be undone"} if current_club_player == listing.club_player %></li>
				  </ul>
			  </div>
		  </div> 
     </div>
    <% end %>
</div>